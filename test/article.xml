                    </secondary>
                </msc>
            </ams-meta-group>
            <contrib-group content-type="contribAs">
                <contrib contrib-type="contribA">
                    <contrib-id contrib-id-type="mrauth">mrauthid</contrib-id>
                    <contrib-id contrib-id-type="orcid">orcidid</contrib-id>
